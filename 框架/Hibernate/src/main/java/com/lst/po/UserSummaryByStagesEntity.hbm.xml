<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lst.po.UserSummaryByStagesEntity" table="user_summary_by_stages" schema="sys">
        <property name="user">
            <column name="user" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="eventName">
            <column name="event_name" sql-type="varchar(128)" length="128"/>
        </property>
        <property name="total">
            <column name="total" sql-type="bigint(20) unsigned"/>
        </property>
        <property name="totalLatency">
            <column name="total_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="avgLatency">
            <column name="avg_latency" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>