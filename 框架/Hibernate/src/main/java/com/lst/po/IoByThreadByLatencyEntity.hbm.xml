<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lst.po.IoByThreadByLatencyEntity" table="io_by_thread_by_latency" schema="sys">
        <property name="user">
            <column name="user" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="total">
            <column name="total" sql-type="decimal(42)" precision="42" not-null="true"/>
        </property>
        <property name="totalLatency">
            <column name="total_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="minLatency">
            <column name="min_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="avgLatency">
            <column name="avg_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="maxLatency">
            <column name="max_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="threadId">
            <column name="thread_id" sql-type="bigint(20) unsigned"/>
        </property>
        <property name="processlistId">
            <column name="processlist_id" sql-type="bigint(20) unsigned" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>