<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lst.po.VersionEntity" table="version" schema="sys">
        <property name="sysVersion">
            <column name="sys_version" sql-type="varchar(5)" length="5"/>
        </property>
        <property name="mysqlVersion">
            <column name="mysql_version" sql-type="varchar(10)" length="10"/>
        </property>
    </class>
</hibernate-mapping>