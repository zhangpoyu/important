<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lst.po.X$MemoryGlobalByCurrentBytesEntity" table="x$memory_global_by_current_bytes" schema="sys">
        <property name="eventName">
            <column name="event_name" sql-type="varchar(128)" length="128"/>
        </property>
        <property name="currentCount">
            <column name="current_count" sql-type="bigint(20)"/>
        </property>
        <property name="currentAlloc">
            <column name="current_alloc" sql-type="bigint(20)"/>
        </property>
        <property name="currentAvgAlloc">
            <column name="current_avg_alloc" sql-type="decimal(23,4)" precision="23" scale="4"/>
        </property>
        <property name="highCount">
            <column name="high_count" sql-type="bigint(20)"/>
        </property>
        <property name="highAlloc">
            <column name="high_alloc" sql-type="bigint(20)"/>
        </property>
        <property name="highAvgAlloc">
            <column name="high_avg_alloc" sql-type="decimal(23,4)" precision="23" scale="4"/>
        </property>
    </class>
</hibernate-mapping>