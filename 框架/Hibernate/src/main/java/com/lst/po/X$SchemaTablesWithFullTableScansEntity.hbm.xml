<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lst.po.X$SchemaTablesWithFullTableScansEntity" table="x$schema_tables_with_full_table_scans"
           schema="sys">
        <property name="objectSchema">
            <column name="object_schema" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="objectName">
            <column name="object_name" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="rowsFullScanned">
            <column name="rows_full_scanned" sql-type="bigint(20) unsigned"/>
        </property>
        <property name="latency">
            <column name="latency" sql-type="bigint(20) unsigned"/>
        </property>
    </class>
</hibernate-mapping>