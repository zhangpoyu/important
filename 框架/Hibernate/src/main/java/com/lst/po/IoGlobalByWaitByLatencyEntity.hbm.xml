<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lst.po.IoGlobalByWaitByLatencyEntity" table="io_global_by_wait_by_latency" schema="sys">
        <property name="eventName">
            <column name="event_name" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="total">
            <column name="total" sql-type="bigint(20) unsigned"/>
        </property>
        <property name="totalLatency">
            <column name="total_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="avgLatency">
            <column name="avg_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="maxLatency">
            <column name="max_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="readLatency">
            <column name="read_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="writeLatency">
            <column name="write_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="miscLatency">
            <column name="misc_latency" sql-type="text" not-null="true"/>
        </property>
        <property name="countRead">
            <column name="count_read" sql-type="bigint(20) unsigned"/>
        </property>
        <property name="totalRead">
            <column name="total_read" sql-type="text" not-null="true"/>
        </property>
        <property name="avgRead">
            <column name="avg_read" sql-type="text" not-null="true"/>
        </property>
        <property name="countWrite">
            <column name="count_write" sql-type="bigint(20) unsigned"/>
        </property>
        <property name="totalWritten">
            <column name="total_written" sql-type="text" not-null="true"/>
        </property>
        <property name="avgWritten">
            <column name="avg_written" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>