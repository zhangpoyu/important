<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lst.po.HostSummaryByFileIoEntity" table="host_summary_by_file_io" schema="sys">
        <id name="id"/>
        <property name="host">
            <column name="host" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="ios">
            <column name="ios" sql-type="decimal(42)" precision="42" not-null="true"/>
        </property>
        <property name="ioLatency">
            <column name="io_latency" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>