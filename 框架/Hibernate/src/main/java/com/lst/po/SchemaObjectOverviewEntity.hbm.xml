<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lst.po.SchemaObjectOverviewEntity" table="schema_object_overview" schema="sys">
        <property name="db">
            <column name="db" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="objectType">
            <column name="object_type" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="count">
            <column name="count" sql-type="bigint(21)"/>
        </property>
    </class>
</hibernate-mapping>