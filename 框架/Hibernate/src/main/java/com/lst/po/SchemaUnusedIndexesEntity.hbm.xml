<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lst.po.SchemaUnusedIndexesEntity" table="schema_unused_indexes" schema="sys">
        <property name="objectSchema">
            <column name="object_schema" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="objectName">
            <column name="object_name" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="indexName">
            <column name="index_name" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>